{
  "name": "react-snowpack-tailwind",
  "private": true,
  "scripts": {
    "build:css": "postcss ./src/style.css -o ./src/compiled.css",
    "start": "yarn build:css && snowpack dev",
    "build": "NODE_ENV=production npm run build:css && snowpack build",
    "test": "jest",
    "lint": "eslint -c .eslintrc.js 'src/**/*.ts*'",
    "format": "eslint -c .eslintrc.js --fix 'src/**/*.ts*'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@snowpack/app-scripts-react": "^2.0.0",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-react-refresh": "^2.3.8",
    "@snowpack/plugin-typescript": "^1.1.1",
    "@snowpack/plugin-webpack": "^2.2.0",
    "@storybook/addon-actions": "6.1.16",
    "@storybook/addon-controls": "^6.1.16",
    "@storybook/addon-essentials": "^6.1.16",
    "@storybook/addon-knobs": "^6.1.16",
    "@storybook/addon-links": "^6.1.16",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.16",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.0.0",
    "@types/chai": "^4.2.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/snowpack-env": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "jest": "^26.6.3",
    "postcss-cli": "^8.3.1",
    "prettier": "2.2.1",
    "snowpack": "3.2",
    "typescript": "^4.0.0"
  }
}
